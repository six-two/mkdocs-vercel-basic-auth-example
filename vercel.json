{
    "routes": [
        { "src": "/.*", "dest": "index.js" }
    ],
    "functions": {
        "index.js": {
            "memory": 128,
            "maxDuration": 5
        }
    },
    "buildCommand": "bash -x ./vercel-build.sh",
    "installCommand": "echo 'Installation is handled by the build script'",
    "outputDirectory": "public",
    "github": {
        "silent": true
    }
}